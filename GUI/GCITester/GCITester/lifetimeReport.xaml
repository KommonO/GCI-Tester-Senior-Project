<Window x:Class="GCITester.lifetimeReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GCITester"
        mc:Ignorable="d"
        Title="lifetimeReport" Height="553.2" Width="1372.418">
    <Grid Margin="0,0,-6.4,0.4">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="71*"/>
            <ColumnDefinition Width="417*"/>
            <ColumnDefinition Width="60*"/>
            <ColumnDefinition Width="555*"/>
        </Grid.ColumnDefinitions>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="420" Margin="10,38,0,0" VerticalAlignment="Top" Width="656" ItemsSource="{Binding}" SelectionChanged="dataGrid_SelectionChanged" Grid.Column="3"/>
        <Button x:Name="Legacy" Content="Pull Data" HorizontalAlignment="Left" Margin="42,474,0,0" VerticalAlignment="Top" Width="123" Background="#FF15315F" Foreground="#FFFDFDFD" FontSize="14" FontWeight="Bold" FontFamily="Microsoft JhengHei UI" Click="Legacy_Click" Grid.ColumnSpan="2"/>
        <ListBox HorizontalAlignment="Left" Height="327" Margin="15,131,0,0" VerticalAlignment="Top" Width="123" Grid.ColumnSpan="2"/>
        <ListBox HorizontalAlignment="Left" Height="327" Margin="64,131,0,0" VerticalAlignment="Top" Width="122" Grid.Column="1"/>
        <ListBox HorizontalAlignment="Left" Height="327" Margin="383,131,0,0" VerticalAlignment="Top" Width="200" Grid.Column="1" RenderTransformOrigin="0.478,0.545" Grid.ColumnSpan="2" Background="#FFFDF6F6" />
        <Image x:Name="image" HorizontalAlignment="Left" Height="45" Margin="10,10,0,0" VerticalAlignment="Top" Width="183" Source="GCILogo.jpg" RenderTransformOrigin="0.751,0.444" Grid.ColumnSpan="2"/>
        <TextBox x:Name="batchName_textBox" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="23" Margin="15,104,0,0" TextWrapping="Wrap" Text="Filter" VerticalAlignment="Top" Width="123" RenderTransformOrigin="0.577,1.696"/>
        <TextBox x:Name="partName_textBox" Grid.Column="1" HorizontalAlignment="Left" Height="23" Margin="64,108,0,0" TextWrapping="Wrap" Text="Filter" VerticalAlignment="Top" Width="122"/>
        <TextBox x:Name="filterFiles_textBox" Grid.Column="1" HorizontalAlignment="Left" Height="23" Margin="383,104,0,0" TextWrapping="Wrap" Text="Filter" VerticalAlignment="Top" Width="200" Grid.ColumnSpan="2"/>
        <Button x:Name="toExcel_button" Content="Export to Excel" Grid.Column="3" HorizontalAlignment="Left" Height="22" Margin="10,474,0,0" VerticalAlignment="Top" Width="129" Background="#FF15315F" Foreground="#FFFDFDFD" FontSize="14" FontWeight="Bold" FontFamily="Microsoft JhengHei UI" />
        <Label x:Name="partName_label" Grid.ColumnSpan="2" Content="Part Name" HorizontalAlignment="Left" Height="30" Margin="15,86,0,0" VerticalAlignment="Top" Width="96"/>
        <Label x:Name="batchName_label" Content="Batch Name" Grid.Column="1" HorizontalAlignment="Left" Height="30" Margin="64,86,0,0" VerticalAlignment="Top" Width="94"/>
        <Label x:Name="reportAvail_label" Grid.ColumnSpan="2" Content="Reports Available" Grid.Column="1" HorizontalAlignment="Left" Height="30" Margin="383,74,0,0" VerticalAlignment="Top" Width="174"/>
    </Grid>
</Window>
