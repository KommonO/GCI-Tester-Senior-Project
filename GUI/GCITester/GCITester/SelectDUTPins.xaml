<Window x:Class="GCITester.SelectDUTPins"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GCITester"
        mc:Ignorable="d"
        Title="SelectDUTPins" Height="198.81" Width="452.381" Loaded="SelectDUTPins_Load" Closed="SelectDUTPins_Closed">
    <Grid>
        <Label x:Name="label" Content="Enter Number of Pins" HorizontalAlignment="Left" Margin="0,61,0,0" VerticalAlignment="Top" Height="32" Width="132"/>
        <TextBox x:Name="numberofPins" HorizontalAlignment="Left" Height="37" Margin="137,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="138" BorderThickness="5" FontSize="20" BorderBrush="#FF15315F"/>
        <Button x:Name="buttonAccept" Content="Add" HorizontalAlignment="Left" Margin="306,57,0,0" VerticalAlignment="Top" Width="100" Height="36" Background="#FF15315F" Foreground="#FFFDFDFD" FontSize="20" FontWeight="Bold" FontFamily="Microsoft JhengHei UI" BorderBrush="#FF15315F" Click="buttonAccept_Click"/>

    </Grid>
</Window>
